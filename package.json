  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "falconjsx": "file:devtools/falconjsx"
  },
  "dependencies": {
    "flyd": "^0.2.8"
  }
}