{
  "name": "falcon",
  "version": "0.2.0",
  "description": "Fast Frontend Framework",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./demo/index.html --port 9999 --open --no-cache --out-dir ./tmp",
    "dev": "parcel ./demo/scratch.html --port 3000 --open --no-cache --out-dir ./tmp",
    "build": "rm -rf dist && parcel build ./src/index.js"
  },
  "keywords": ["frontend", "virtual dom", "vdom", "falcon", "front end"],
